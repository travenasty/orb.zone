  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-typecheck": "^1.2.0",
