    "chai-virtual-dom": "^1.0.0",
    "clean-webpack-plugin": "^0.1.3",
    "compression-webpack-plugin": "^0.2.0",
    "eslint": "^1.3.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-no-class": "^0.1.0",
    "eslint-plugin-react": "^3.3.0",
    "html-webpack-plugin": "^1.6.1",
    "node-libs-browser": "^0.5.2",
    "postcss": "^5.0.6",
    "uglify-js": "2.4.24",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.1.3"
  },
  "scripts": {
    "install": "npm install --global postcss-cli autoprefixer",
    "reset": "node ./manage/reset_redis_db",
    "build-debug": "mkdir -p public/js && mkdir -p public/css && browserify src/app.js -t babelify --outfile public/js/app.js",
    "build-css": "postcss --use autoprefixer src/app.css -d public/css/",
    "build-worker": "browserify src/workers/ozm.js -t babelify --outfile public/js/osm.js",
    "uglify": "uglifyjs public/js/app.js -o public/js/app.min.js",
    "build": "npm run build-debug && npm run build-css && npm run uglify",
    "compile": "env NODE_ENV=production webpack --config webpack-prod.config.babel.js --progress",
    "lint": "eslint .",
    "start": "webpack-dev-server --config webpack-dev.config.babel.js --progress",
    "test": "npm run lint"
  }
}
